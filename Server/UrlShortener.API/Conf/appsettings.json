{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "UrlGenerationOptions": {
    "Characters": "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz",
    "Length": 15
  },
  "ConnectionStrings": {
    "UrlShortenContext": "Server=host.docker.internal;Database=UrlShortener;User Id={0}; Password={1};TrustServerCertificate=true"
  }
}
